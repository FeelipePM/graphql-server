generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
 
model User {
  id          String @id @db.Uuid @default(uuid())
  name        String
  email       String 
  password    String
  birthDate   String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @default(now())

  @@map("users")
}